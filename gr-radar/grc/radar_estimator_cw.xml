<block>
  <name>Estimator CW</name>
  <key>radar_estimator_cw</key>
  <category>[RADAR]/Estimators</category>
  <import>import radar</import>
  <make>radar.estimator_cw($center_freq)</make>
  <param>
    <name>Center frequency</name>
    <key>center_freq</key>
    <type>float</type>
  </param>
  <sink>
    <name>Msg in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>Msg out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
