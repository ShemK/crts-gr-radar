<block>
  <name>QT GUI Spectrogram Plot</name>
  <key>radar_qtgui_spectrogram_plot</key>
  <category>[RADAR]/GUI</category>
  <import>import radar</import>
  <make>radar.qtgui_spectrogram_plot($vlen, $interval, $xlabel, $ylabel, $label, $axis_x, $axis_y, $axis_z, $autoscale_z, $len_key)</make>
  <param>
    <name>Input vector length</name>
    <key>vlen</key>
    <type>int</type>
  </param>
  <param>
    <name>Update interval [ms]</name>
    <key>interval</key>
    <type>int</type>
  </param>
  <param>
    <name>Label x</name>
    <key>xlabel</key>
    <type>string</type>
  </param>
  <param>
    <name>Label y</name>
    <key>ylabel</key>
    <type>string</type>
  </param>
  <param>
    <name>Label title</name>
    <key>label</key>
    <type>string</type>
  </param>
  <param>
    <name>Axis x</name>
    <key>axis_x</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Axis y</name>
    <key>axis_y</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Axis z</name>
    <key>axis_z</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Autoscale z axis</name>
    <key>autoscale_z</key>
    <value>True</value>
    <type>bool</type>
      <option>
		<name>True</name>
		<key>True</key>
	  </option>
	  <option>
		<name>False</name>
		<key>False</key>
	  </option>
  </param>
  <param>
    <name>Packet length key</name>
    <key>len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>float</type>
    <vlen>$vlen</vlen>
  </sink>
</block>
