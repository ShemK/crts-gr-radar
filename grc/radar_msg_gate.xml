<block>
  <name>Message Gate</name>
  <key>radar_msg_gate</key>
  <category>[RADAR]/Tools</category>
  <import>import radar</import>
  <make>radar.msg_gate($msg_keys, $val_min, $val_max)</make>
  <param>
    <name>Keys</name>
    <key>msg_keys</key>
    <type>raw</type>
  </param>
  <param>
    <name>Value min</name>
    <key>val_min</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Value max</name>
    <key>val_max</key>
    <type>real_vector</type>
  </param>
  <sink>
    <name>Msg in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>Msg out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
