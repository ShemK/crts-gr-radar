<block>
  <name>Trigger Command</name>
  <key>radar_trigger_command</key>
  <category>[RADAR]/Tools</category>
  <import>import radar</import>
  <make>radar.trigger_command($command, $identifiers, $vals_min, $vals_max, $block_time)</make>
  <param>
    <name>Command</name>
    <key>command</key>
    <type>string</type>
  </param>
  <param>
    <name>Identifiers</name>
    <key>identifiers</key>
    <type>raw</type>
  </param>
  <param>
    <name>Value min</name>
    <key>vals_min</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Value max</name>
    <key>vals_max</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Block duration [ms]</name>
    <key>block_time</key>
    <value>0</value>
    <type>int</type>
  </param>
  <sink>
    <name>Msg in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
</block>
