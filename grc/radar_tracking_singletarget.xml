<block>
  <name>Tracking Single Target</name>
  <key>radar_tracking_singletarget</key>
  <category>[RADAR]/RADAR</category>
  <import>import radar</import>
  <make>radar.tracking_singletarget($num_particle, $std_range_meas, $std_velocity_meas, $std_accel_sys, $threshold_track, $threshold_lost, $filter)</make>
  <param>
    <name>Particle number</name>
    <key>num_particle</key>
    <type>int</type>
  </param>
  <param>
    <name>Std range measurement</name>
    <key>std_range_meas</key>
    <type>float</type>
  </param>
  <param>
    <name>Std velocity measurement</name>
    <key>std_velocity_meas</key>
    <type>float</type>
  </param>
  <param>
    <name>Std system acceleration</name>
    <key>std_accel_sys</key>
    <type>float</type>
  </param>
  <param>
    <name>Threshold track acceptance</name>
    <key>threshold_track</key>
    <type>float</type>
  </param>
  <param>
    <name>Threshold lost track</name>
    <key>threshold_lost</key>
    <type>int</type>
  </param>
  <param>
    <name>Filter</name>
    <key>filter</key>
    <type>string</type>
  </param>
  <sink>
    <name>Msg in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>Msg out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
