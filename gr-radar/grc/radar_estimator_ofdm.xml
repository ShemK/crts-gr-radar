<block>
  <name>Estimator OFDM</name>
  <key>radar_estimator_ofdm</key>
  <category>[RADAR]/Estimators</category>
  <import>import radar</import>
  <make>radar.estimator_ofdm($symbol_x, $len_x, $axis_x, $symbol_y, $len_y, $axis_y, $merge_consecutive)</make>
  <param>
    <name>Symbol x</name>
    <key>symbol_x</key>
    <type>string</type>
  </param>
  <param>
    <name>Length x</name>
    <key>len_x</key>
    <type>int</type>
  </param>
  <param>
    <name>Axis x</name>
    <key>axis_x</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Symbol y</name>
    <key>symbol_y</key>
    <type>string</type>
  </param>
  <param>
    <name>Length y</name>
    <key>len_y</key>
    <type>int</type>
  </param>
  <param>
    <name>Axis y</name>
    <key>axis_y</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Merge consecutive peaks</name>
    <key>merge_consecutive</key>
    <value>True</value>
    <type>bool</type>
      <option>
		<name>True</name>
		<key>True</key>
	  </option>
	  <option>
		<name>False</name>
		<key>False</key>
	  </option>
  </param>
  <sink>
    <name>Msg in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>Msg out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
