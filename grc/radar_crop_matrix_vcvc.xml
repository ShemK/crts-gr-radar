<block>
  <name>Matrix Crop</name>
  <key>radar_crop_matrix_vcvc</key>
  <category>[RADAR]/Tools</category>
  <import>import radar</import>
  <make>radar.crop_matrix_vcvc($vlen, $crop_x, $crop_y, $len_key)</make>
  <param>
    <name>Vector length</name>
    <key>vlen</key>
    <type>int</type>
  </param>
  <param>
    <name>Crop x</name>
    <key>crop_x</key>
    <type>int_vector</type>
  </param>
  <param>
    <name>Crop y</name>
    <key>crop_y</key>
    <type>int_vector</type>
  </param>
  <param>
    <name>Packet length key</name>
    <key>len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>complex</type>
    <vlen>$vlen</vlen>
  </sink>
  <source>
    <name>IQ out</name>
    <type>complex</type>
    <vlen>($crop_x)[1]-($crop_x)[0]</vlen>
  </source>
</block>
