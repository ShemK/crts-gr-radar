<block>
  <name>Find Max Peak</name>
  <key>radar_find_max_peak_c</key>
  <category>[RADAR]/Tools</category>
  <import>import radar</import>
  <make>radar.find_max_peak_c($samp_rate, $threshold, $samp_protect, $max_freq, $cut_max_freq, $len_key)</make>
  <callback>set_threshold($threshold)</callback>
  <callback>set_samp_protect($samp_protect)</callback>
  <param>
    <name>Sample rate</name>
    <key>samp_rate</key>
    <type>int</type>
  </param>
  <param>
    <name>Threshold [dB]</name>
    <key>threshold</key>
    <type>float</type>
  </param>
  <param>
    <name>Number protected samples</name>
    <key>samp_protect</key>
    <type>int</type>
  </param>
  <param>
    <name>Cut frequencies</name>
    <key>max_freq</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Use cut frequencies</name>
    <key>cut_max_freq</key>
    <value>False</value>
    <type>bool</type>
      <option>
		<name>True</name>
		<key>True</key>
	  </option>
	  <option>
		<name>False</name>
		<key>False</key>
	  </option>
  </param>
  <param>
    <name>Packet length key</name>
    <key>len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>Msg out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
