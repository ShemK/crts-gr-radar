<block>
  <name>Tagged Stream FFT</name>
  <key>radar_ts_fft_cc</key>
  <category>[RADAR]/Tools</category>
  <import>import radar</import>
  <make>radar.ts_fft_cc($packet_len, $len_key)</make>
  <param>
    <name>Packet length</name>
    <key>packet_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Packet length key</name>
    <key>len_key</key>
    <value> "packet_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>IQ out</name>
    <type>complex</type>
  </source>
</block>
